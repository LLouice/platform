{
    "compilerOptions": {
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "module": "es5",
        "target": "ES2015",
        "moduleResolution": "node",
        "declaration": false,
        "noImplicitAny": false,
        "noEmitOnError": true,
        "removeComments": true,
        "sourceMap": true,
        "noLib": false,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "outDir": "./dist_ts/"
    }
}
